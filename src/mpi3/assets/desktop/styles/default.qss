    text-align: left;
    font-family: "Open Sans";
    font-size: 16px;
    font-weight: bold;
    margin: 12px 0px -2px 0px;
    padding: 0px 0px 0px 0px;
}


QWidget#PanelSection {
    background: #404040;
    border: 1px solid #FFFFFF;
    border-radius: 3px;
}


QWidget#PanelDisplay {
    background: #222222;
    border-left: 1px solid #696969;
    margin: 0px;
}


QWidget#PanelControl {
    background: #191919;
    border: 0px;
    margin: 0px;
}


QWidget#StorageWidget {
    background: #FFFFFF;
